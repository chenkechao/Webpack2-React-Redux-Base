    "webpack-dev-server": "^2.1.0-beta",
    "webpack-md5-hash": "0.0.5",
    "webpack-validator": "^2.2.3"
  },
  "dependencies": {
    "es6-promise": "^3.3.1",
    "express": "^4.14.0",
    "fluent-fetcher": "^0.1.2",
    "font-awesome": "^4.6.3",
    "install": "^0.8.1",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.1.0",
    "js-cookie": "^2.1.3",
    "lodash": "^4.15.0",
    "normalize.css": "^4.2.0",
    "npm": "^3.10.8",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-fontawesome": "^1.2.0",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.6.0",
    "redux-form": "^6.0.5",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "scroll-behavior": "^0.8.2",
    "whatwg-fetch": "^1.0.0"
  }
}
